  "type": "library",
  "license": "MIT",
  "minimum-stability": "dev",
  "authors": [
    {
      "name": "Puleeno Nguyen",
      "email": "puleeno@gmail.com"
    }
  ],
  "require": {
    "jankx/admin": "dev-master",
    "jankx/field": "dev-master",
    "jankx/option": "dev-master",
    "jankx/asset": "dev-master",
    "jankx/template": "dev-master",
    "jankx/ui-framework": "dev-master",
    "jankx/data-type": "dev-master",
    "jankx/human-number": "dev-master"
